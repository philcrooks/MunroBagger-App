{
  "name": "munro_bagger",
  "version": "1.0.0",
  "description": "A JS/ReactJS/Rails website that allows the user to see all the sunny Munros in Scotland at one glance.",
  "main": "seeds.js",
  "scripts": {
    "test": "mocha ./client/src/specs/",
    "start": "node server.js",
    "bundle": "cd client && webpack -w",
    "bundle-p": "cd client && webpack -p",
    "uglify": "cd www && uglifycss css/material.css > css/material.min.css && uglifycss css/style.css > css/style.min.css && uglifyjs js/bundle.js -c -m -o js/bundle.min.js"
  },
  "author": {
    "name": "Munro Bagger Team",
    "url": "https://www.munrobagger.scot"
  },
  "contributors": [
    {
      "name": "Phil Crooks",
      "email": "coder.crooks@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:philcrooks/MunroBagger-App.git"
  },
  "homepage": "http://www.munrobagger.scot",
  "license": "ISC",
  "dependencies": {
    "react": "^15.3.2",
    "react-datepicker": "^0.33.0",
    "react-dom": "^15.3.2",
    "react-mdl": "^1.9.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "google-maps-utility-library-v3-infobox": "^1.1.14",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3"
  }
}
